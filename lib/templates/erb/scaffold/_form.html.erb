<%# frozen_string_literal: true %>
<%= simple_form_for(@gym) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :pricing %>


  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
